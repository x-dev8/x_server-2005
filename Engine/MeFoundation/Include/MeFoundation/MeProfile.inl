//////////////////////////////////////////////////////////////////////////
template<class T>
inline void MeProfile<T>::Start()
{
	m_kTimer.Start();
}
//////////////////////////////////////////////////////////////////////////
template<class T>
inline void MeProfile<T>::End()
{
	m_kTimer.End();
}
//////////////////////////////////////////////////////////////////////////
template<class T>
inline MeTimeValue MeProfile<T>::GetElapsed() const
{
	return m_kTimer.GetElapsed();
}
